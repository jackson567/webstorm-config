<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="$ShowTranslationDialogAction" />
  <action id="$TranslateAndReplaceAction" />
  <action id="EditorDeleteLine" />
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f" />
  </action>
  <action id="osmedile.intellij.stringmanip.PopupChoiceAction">
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="osmedile.intellij.stringmanip.SwitchCasePopup">
    <keyboard-shortcut first-keystroke="ctrl alt comma" />
  </action>
  <action id="tasks.switch">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
</keymap>